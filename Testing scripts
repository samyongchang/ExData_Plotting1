data <- read.table("data.txt", header=TRUE, sep=";", na.strings="?")
data$Date <- as.Date(data$Date, format="%d/%m/%Y")
data1 <- subset(data, data$Date >= "2007/02/01" & data$Date <="2007/02/02")


  plot(data1$Sub_metering_1~data1$datetime, type="l")
  lines(data1$Sub_metering_3~data1$datetime, col="blue")
  lines(data1$Sub_metering_2~data1$datetime, col="red")


legend("topright", col=c("black","red","blue"), lty=1, lwd=2, legend=c("x","t","r"))

legend("topright", col=c("black", "red", "blue"), lty=1, lwd=2,
legend=c("aaa", "aaaq", "aasad"))
  
